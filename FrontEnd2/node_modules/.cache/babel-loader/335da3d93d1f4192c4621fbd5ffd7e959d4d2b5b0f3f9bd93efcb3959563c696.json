{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Layout from '@/components/Layout.vue';\nimport request from \"@/utils/request\";\nimport { formatDate } from \"@/utils/tool.js\";\nexport default {\n  name: 'LunTan',\n  components: {\n    Layout\n  },\n  data() {\n    return {\n      activeTab: 'tab1',\n      // 默认激活的选项卡\n      isCollapse: false,\n      asideWidth: '200px',\n      collapseIcon: 'el-icon-s-fold',\n      query: {\n        teacher_Name: '',\n        teacher_ID: '',\n        teacher_Subject: ''\n      },\n      tableData: [],\n      index: 0,\n      scheme: {}\n    };\n  },\n  mounted() {},\n  created() {\n    // 在组件创建后立即发送请求\n    this.fetchData();\n  },\n  methods: {\n    handleTabClick(tab) {\n      console.log(`当前激活的选项卡: ${tab.name}`);\n    },\n    fetchData() {\n      console.log(\"fetchData111\");\n      request.get('/tutorial/appointments/get').then(res => {\n        // console.log(\"交互成功\")\n        console.log(\"fetchDataDone\");\n        console.log(res);\n        if (res.status === 200) {\n          // console.log(\"收到后端回复200\")\n\n          this.$message.success(res.msg);\n          let obj = res.data;\n          this.tableData = obj;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleFullScreen() {\n      document.documentElement.requestFullscreen();\n    },\n    handleCollapse() {\n      this.isCollapse = !this.isCollapse;\n      this.asideWidth = this.isCollapse ? '64px' : '200px';\n      this.collapseIcon = this.isCollapse ? 'el-icon-s-unfold' : 'el-icon-s-fold';\n    },\n    toMenu() {\n      this.$router.push('/fudao');\n    },\n    handleDelete(index, row) {\n      // 前端删除\n      this.tableData.splice(index, 1);\n\n      // 后端删除\n      let appointment_id = row.appointment_id;\n      request.post('/tutorial/appointments/delete', {\n        appointment_id: appointment_id\n      }).then(res => {\n        console.log(res);\n        if (res.status === 200) {\n          this.$message.success(res.msg);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["Layout","request","formatDate","name","components","data","activeTab","isCollapse","asideWidth","collapseIcon","query","teacher_Name","teacher_ID","teacher_Subject","tableData","index","scheme","mounted","created","fetchData","methods","handleTabClick","tab","console","log","get","then","res","status","$message","success","msg","obj","error","handleFullScreen","document","documentElement","requestFullscreen","handleCollapse","toMenu","$router","push","handleDelete","row","splice","appointment_id","post"],"sources":["src/views/FuDao/AppointmentList.vue"],"sourcesContent":["<template>\r\n  <Layout>\r\n\r\n        <el-main>\r\n          <el-card style=\" margin-bottom: 10px\" >\r\n              <div slot=\"header\" class=\"clearfix\">\r\n                  <span>我的预约</span>\r\n              </div>\r\n\r\n              <el-form :inline=\"true\" :model=\"query\" class=\"demo-form-inline\">\r\n                  <el-form-item label=\"辅导师姓名\">\r\n                      <el-input v-model=\"query.teacher_Name\" placeholder=\"请输入辅导师姓名\"></el-input>\r\n                  </el-form-item>\r\n                  <el-form-item label=\"辅导师工号\">\r\n                      <el-input v-model=\"query.teacher_ID\" placeholder=\"请输入辅导师工号\"></el-input>\r\n                  </el-form-item>\r\n                  <el-form-item label=\"辅导师学科\">\r\n                      <el-input v-model=\"query.teacher_Subject\" placeholder=\"请输入辅导师学科\"></el-input>\r\n                  </el-form-item>\r\n                  <el-form-item>\r\n                      <el-button type=\"primary\" @click=\"onSubmit\">查询</el-button>\r\n                  </el-form-item>\r\n                  <el-form-item>\r\n                      <el-button type=\"danger\" @click=\"toMenu\">返回</el-button>\r\n                  </el-form-item>\r\n              </el-form>\r\n              \r\n\r\n              <el-tabs v-model=\"activeTab\" @tab-click=\"handleTabClick\">\r\n                <el-tab-pane label=\"一对一\" name=\"tab1\"></el-tab-pane>\r\n                <el-tab-pane label=\"习题课\" name=\"tab2\"></el-tab-pane>\r\n\r\n              </el-tabs>\r\n\r\n              <div v-if=\"activeTab === 'tab1'\">\r\n                <h2>这是选项卡 1 的内容</h2>\r\n                <p>这里可以放任何你想展示的内容。</p>\r\n              </div>\r\n\r\n              <div v-if=\"activeTab === 'tab2'\">\r\n\r\n                <el-table\r\n                  :data=\"tableData\"\r\n                  stripe\r\n                  style=\"width: 100%; margin-bottom: 10px\">\r\n                  <el-table-column\r\n                    type=index\r\n                    label=\"序号\"\r\n                    width=\"150\"\r\n                    :align=\"'center'\">\r\n                    \r\n                  </el-table-column>\r\n\r\n                  <el-table-column\r\n                    prop=\"tutor_id\"\r\n                    label=\"老师编号\"\r\n                    width=\"200\"\r\n                    :align=\"'center'\">\r\n                  </el-table-column>\r\n\r\n                  <el-table-column\r\n                    prop=\"tutor_name\"\r\n                    label=\"老师姓名\"\r\n                    width=\"200\"\r\n                    :align=\"'center'\">\r\n                  </el-table-column>\r\n\r\n                  <el-table-column\r\n                    prop=\"appointment_time_start\"\r\n                    label=\"开始时间\"\r\n                    width=\"300\"\r\n                    :align=\"'center'\">\r\n                  </el-table-column>\r\n\r\n                  <el-table-column\r\n                    prop=\"appointment_time_end\"\r\n                    label=\"结束时间\"\r\n                    width=\"300\"\r\n                    :align=\"'center'\">\r\n                  </el-table-column>\r\n                  \r\n                  <el-table-column\r\n                    label=\"操作\"\r\n  