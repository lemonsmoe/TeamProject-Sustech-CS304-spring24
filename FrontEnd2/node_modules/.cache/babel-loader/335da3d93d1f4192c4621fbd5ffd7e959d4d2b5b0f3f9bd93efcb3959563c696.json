n          this.$message.error(res.msg)\r\n        }\r\n        \r\n      })\r\n\r\n    },\r\n    handleFullScreen() {\r\n      document.documentElement.requestFullscreen()\r\n    },\r\n    handleCollapse() {\r\n      this.isCollapse = !this.isCollapse\r\n      this.asideWidth = this.isCollapse ? '64px' : '200px'\r\n      this.collapseIcon = this.isCollapse ? 'el-icon-s-unfold' : 'el-icon-s-fold'\r\n    },\r\n    \r\n    toMenu() {\r\n      this.$router.push('/fudao')\r\n    },\r\n    handleDelete(index, row) {\r\n      // 前端删除\r\n      this.tableData.splice(index, 1)\r\n      \r\n      // 后端删除\r\n      let appointment_id = row.appointment_id\r\n      request.post('/tutorial/appointments/delete', {\r\n        appointment_id: appointment_id\r\n      }).then(res => {\r\n        console.log(res)\r\n        if (res.status === 200) {\r\n          this.$message.success(res.msg)\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    }, \r\n    \r\n  }\r\n}\r\n</script>"],"mappings":";AA6GA,OAAAA,MAAA;AACA,OAAAC,OAAA;AACA,SAAAC,UAAA;AACA;EACAC,IAAA;EACAC,UAAA;IACAJ;EACA;EACAK,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,UAAA;MACAC,UAAA;MACAC,YAAA;MAEAC,KAAA;QACAC,YAAA;QACAC,UAAA;QACAC,eAAA;MACA;MAEAC,SAAA;MAEAC,KAAA;MACAC,MAAA;IAEA;EACA;EACAC,QAAA,GAEA;EACAC,QAAA;IACA;IACA,KAAAC,SAAA;EACA;EACAC,OAAA;IACAC,eAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,cAAAF,GAAA,CAAAnB,IAAA;IACA;IACAgB,UAAA;MACAI,OAAA,CAAAC,GAAA;MACAvB,OAAA,CAAAwB,GAAA,+BAAAC,IAAA,CAAAC,GAAA;QACA;QACAJ,OAAA,CAAAC,GAAA;QACAD,OAAA,CAAAC,GAAA,CAAAG,GAAA;QACA,IAAAA,GAAA,CAAAC,MAAA;UACA;;UAEA,KAAAC,QAAA,CAAAC,OAAA,CAAAH,GAAA,CAAAI,GAAA;UACA,IAAAC,GAAA,GAAAL,GAAA,CAAAtB,IAAA;UACA,KAAAS,SAAA,GAAAkB,GAAA;QACA;UACA,KAAAH,QAAA,CAAAI,KAAA,CAAAN,GAAA,CAAAI,GAAA;QACA;MAEA;IAEA;IACAG,iBAAA;MACAC,QAAA,CAAAC,eAAA,CAAAC,iBAAA;IACA;IACAC,eAAA;MACA,KAAA/B,UAAA,SAAAA,UAAA;MACA,KAAAC,UAAA,QAAAD,UAAA;MACA,KAAAE,YAAA,QAAAF,UAAA;IACA;IAEAgC,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACAC,aAAA3B,KAAA,EAAA4B,GAAA;MACA;MACA,KA