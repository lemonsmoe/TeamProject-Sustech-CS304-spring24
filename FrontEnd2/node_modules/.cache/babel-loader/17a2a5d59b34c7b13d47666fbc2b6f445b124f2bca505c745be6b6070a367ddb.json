ollapse\" style=\"font-size: 26px\"></i>\r\n            <!--          <el-breadcrumb separator=\"/\" style=\"margin-left: 20px\">-->\r\n            <!--            <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>-->\r\n            <!--            <el-breadcrumb-item :to=\"{ path: '/' }\">课程管理</el-breadcrumb-item>-->\r\n            <!--          </el-breadcrumb>-->\r\n  \r\n            <div style=\"flex: 1; display: flex; justify-content: flex-end; align-items: center\">\r\n              <i class=\"el-icon-quanping\" @click=\"handleFullScreen\" style=\"font-size: 25px\"></i>\r\n              <el-dropdown placement=\"bottom\">\r\n                <div style=\"display: flex; align-items: center; cursor: pointer\">\r\n                  <img src=\"@/assets/nkd.jpg\" alt=\"\" style=\"width: 40px; height: 40px; margin: 0 5px\">\r\n                  <span>用户中心</span>\r\n                </div>\r\n                <el-dropdown-menu slot=\"dropdown\">\r\n                  <el-dropdown-item>个人信息</el-dropdown-item>\r\n                  <el-dropdown-item>修改密码</el-dropdown-item>\r\n                  <el-dropdown-item>退出登录</el-dropdown-item>\r\n                </el-dropdown-menu>\r\n              </el-dropdown>\r\n            </div>\r\n          </el-header>\r\n\r\n          \r\n  \r\n          <el-main>\r\n            <el-card style=\" margin-bottom: 10px\" >\r\n              <div slot=\"header\" class=\"clearfix\">\r\n                  <span>预约辅导</span>\r\n              </div>\r\n              <el-row>\r\n                <el-col :span=\"8\">\r\n                    <el-card>\r\n                        <img src=\"@/assets/logo.png\" alt=\"\">\r\n                        <div></div>\r\n                        <el-button type=\"primary\" @click=\"toAppointmentList\">我的预约</el-button>\r\n                    </el-card>\r\n                </el-col>\r\n                <el-col :span=\"8\">\r\n                    <el-card>\r\n                        <img src=\"@/assets/logo.png\" alt=\"\">\r\n                        <div></div>\r\n                        <el-button type=\"primary\" @click=\"toTeacherList\">我要预约</el-button>\r\n                    </el-card>\r\n                </el-col>\r\n                <el-col :span=\"8\">\r\n                    <el-card>\r\n                        <img src=\"@/assets/logo.png\" alt=\"\">\r\n                        <div></div>\r\n                        <el-button type=\"primary\">活动查询</el-button>\r\n                    </el-card>\r\n                </el-col>\r\n                  \r\n              </el-row>\r\n                \r\n            </el-card>\r\n            <!-- 三个按钮 第一个跳转到选择辅导师的列表 第二个跳转到预约信息 第三个跳转到心理咨询活动-->\r\n            \r\n            <!-- 选择辅导师的列表 -->\r\n            \r\n            <!-- 点击按钮跳转到辅导师列表页面 -->\r\n            <!-- 辅导师列表页面显示所有辅导师的信息 -->\r\n            \r\n\r\n\r\n            \r\n  \r\n          </el-main>\r\n        </el-container>\r\n      </el-container>\r\n  \r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  \r\n  export default {\r\n    name: 'LunTan',\r\n    data() {\r\n      return {\r\n  \r\n        isCollapse: false,\r\n        asideWidth: '200px',\r\n        collapseIcon: 'el-icon-s-fold',\r\n        form: {\r\n          name: '',\r\n          region: '',\r\n          date1: '',\r\n          date2: '',\r\n          delivery: false,\r\n          type: [],\r\n          resource: '',\r\n          desc: ''\r\n        },\r\n        formInline: {\r\n          xueqi: '',\r\n          mingcheng: '',\r\n          yuanxi: ''\r\n        },\r\n        \r\n        index: 0,\r\n        scheme: {},\r\n        // 老师的信息\r\n\r\n        tableData: [\r\n\r\n\r\n        ],\r\n      }\r\n    },\r\n    mounted() {\r\n  \r\n    },\r\n    \r\n    methods: {\r\n      \r\n      handleFullScreen() {\r\n        document.documentElement.requestFullscreen()\r\n      },\r\n      handleCollapse() {\r\n        this.isCollapse = !this.isCollapse\r\n        this.asideWidth = this.isCollapse ? '64px'