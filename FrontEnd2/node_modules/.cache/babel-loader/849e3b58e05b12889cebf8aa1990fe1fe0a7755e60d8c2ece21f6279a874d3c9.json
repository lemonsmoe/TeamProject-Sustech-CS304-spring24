\n            {\r\n              course_id: 'CS102',\r\n              course_name: '不知道什么课程2',\r\n              course_teacher: '不知道什么老师2',\r\n              course_time: '1-15周单周,星期三第5-6节 2-16周双周,星期三第7-8节',\r\n              course_location: '三教508',\r\n            },\r\n\r\n          ],\r\n  \r\n          addDDL:false,\r\n  \r\n          ddlform: {\r\n            course_id: '',\r\n            ddl_time: '',\r\n            ddl_title: '',\r\n            ddl_content: '',\r\n          },\r\n  \r\n  \r\n\r\n        }\r\n      },\r\n      mounted() {\r\n      \r\n      },\r\n      created() {\r\n        // 在组件创建后立即发送请求\r\n        this.getAllCourse();\r\n  \r\n      },\r\n      methods: {\r\n        getAllCourse() {\r\n          // 获取所有老师信息\r\n          request.get('/course/get').then(res=>{\r\n            console.log(res);\r\n            if (res.status == 200) {\r\n              this.courseData = res.data;\r\n              console.log(res.data);\r\n    \r\n            }\r\n            else{\r\n    \r\n            }\r\n            \r\n          })\r\n        },\r\n        handleAddDDL(index,row) {\r\n          // 前端添加\r\n          this.addDDL = true\r\n\r\n          this.ddlform.course_id === row.course_id\r\n          this.ddlform.ddl_time === '' \r\n          this.ddlform.ddl_title === '' \r\n          this.ddlform.ddl_content === ''\r\n\r\n    \r\n        },\r\n        confirmAddTutor(){\r\n          // 确认添加（更新表格）\r\n  \r\n          //判断是否DDLform所有信息都填写了\r\n          if (this.ddlform.course_id === '' \r\n          || this.ddlform.ddl_time === '' \r\n          || this.ddlform.ddl_title === '' \r\n          || this.ddlform.ddl_content === '') {\r\n            this.$message.error('请填写完整信息')\r\n            return\r\n          }\r\n          \r\n          \r\n    \r\n          const list = {\r\n            'name': this.teacherform.name,\r\n            'subject': this.teacherform.subject,\r\n            'gender': this.teacherform.gender,\r\n            'age': this.teacherform.age,\r\n            'email': this.teacherform.email\r\n          }\r\n    \r\n          request.post('/tutorial/tutors/add', list).then(res=>{\r\n            // console.log(\"交互成功\")\r\n            console.log(\"fetchDataDone\")\r\n            console.log(res)\r\n            if (res.status === 200) {\r\n              this.$message.success('添加成功')\r\n              this.getAllTutor()\r\n            } else {\r\n              this.$message.error(res.data.msg)\r\n            }\r\n            \r\n          })\r\n          \r\n          this.addteacher = false\r\n        },\r\n        \r\n        cancelAddTutor(){\r\n          // 取消添加\r\n          this.addteacher = false\r\n        },\r\n  \r\n        handleDeleteTutor(index, row) {\r\n          // 前端删除\r\n          this.tableData.splice(index, 1)\r\n          \r\n          // 后端删除\r\n          let tutor_id = row.tutor_id\r\n          request.post('/tutorial/tutors/delete', {\r\n            tutor_id: tutor_id\r\n          }).then(res => {\r\n            console.log(res)\r\n            if (res.status === 200) {\r\n              this.$message.success('删除成功')\r\n            } else {\r\n              this.$message.error(res.data.msg)\r\n            }\r\n          })\r\n        }, \r\n  \r\n      },\r\n        \r\n    }\r\n  </script>\r\n  \r\n  <style>\r\n  \r\n  \r\n  </style>\r\n    \r\n    "],"mappings":"AA+CA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,UAAA,GACA;EACAC,KAAA;IACA;MAEAC,UAAA,GACA;QACAC,SAAA;QACAC,WAAA;QACAC,cAAA;QACAC,WAAA;QACAC,eAAA;MACA,GACA;QACAJ,SAAA;QACAC,WAAA;QACAC,cAAA;QACAC,WAAA;QACAC,eAAA;MACA,EAEA;MAEAC,MAAA;MAEAC,OAAA;QACAN,SAAA;QACAO,QAAA;QACAC,SAAA;QACAC,WAAA;MACA;IAIA;EACA;EACAC,QAAA,GAEA;EACAC,QAAA;IACA;IACA,KAAAC,YAAA;EAEA;EACAC,OAAA;IACAD,aAAA;MACA;MACAjB,OAAA,CAAAmB,GAAA,gBAAAC,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA,IAAAA,GAAA,CAAAG,MAAA;UACA,KAAApB,UAAA,GAAAiB,GAAA,CAAAlB,IAAA;UACAmB,OAAA,CAAAC,GAAA,CAAAF,GAAA,CAAAlB,IAAA;QAEA,OACA,CAEA;MAEA;IACA;IACAsB,aAAAC,KAAA,EAAAC,GAAA;MACA;MACA,K