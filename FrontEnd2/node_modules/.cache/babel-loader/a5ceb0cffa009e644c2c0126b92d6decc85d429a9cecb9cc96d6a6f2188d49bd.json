{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Layout from '@/components/Layout.vue';\nimport request from \"@/utils/request\";\nimport { formatDate } from \"@/utils/tool.js\";\nexport default {\n  name: 'LunTan',\n  components: {\n    Layout\n  },\n  data() {\n    return {\n      isCollapse: false,\n      asideWidth: '200px',\n      collapseIcon: 'el-icon-s-fold',\n      query: {\n        teacher_Name: '',\n        teacher_ID: '',\n        teacher_Subject: ''\n      },\n      tableData: [],\n      index: 0,\n      scheme: {}\n    };\n  },\n  mounted() {},\n  created() {\n    // 在组件创建后立即发送请求\n    this.fetchData();\n  },\n  methods: {\n    fetchData() {\n      console.log(\"fetchData111\");\n      request.get('/tutorial/appointments/get').then(res => {\n        // console.log(\"交互成功\")\n        console.log(\"fetchDataDone\");\n        console.log(res);\n        if (res.status === 200) {\n          // console.log(\"收到后端回复200\")\n\n          this.$message.success(res.msg);\n          let obj = res.data;\n          for (let i = 0; i < obj.length; i++) {\n            let list = {\n              // 老师编号 老师姓名 预约起止时间\n              'number': this.tableData.length + 1,\n              'teacher_id': obj[i].tutor_id,\n              'teacher_name': obj[i].tutor_name,\n              'appointment_time_start': formatDate(obj[i].appointment_time_start),\n              'appointment_time_end': formatDate(obj[i].appointment_time_end)\n            };\n            this.tableData.push(list);\n          }\n\n          // console.log(res.data)\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleFullScreen() {\n      document.documentElement.requestFullscreen();\n    },\n    handleCollapse() {\n      this.isCollapse = !this.isCollapse;\n      this.asideWidth = this.isCollapse ? '64px' : '200px';\n      this.collapseIcon = this.isCollapse ? 'el-icon-s-unfold' : 'el-icon-s-fold';\n    },\n    toMenu() {\n      this.$router.push('/fudao');\n    },\n    handleDelete(index, row) {\n      // 删除日程\n      this.tableData.splice(index, 1);\n      // 重新排序\n      this.tableData.forEach((item, index) => {\n        item.number = index + 1;\n      });\n    }\n  }\n};","map":{"version":3,"names":["Layout","request","formatDate","name","components","data","isCollapse","asideWidth","collapseIcon","query","teacher_Name","teacher_ID","teacher_Subject","tableData","index","scheme","mounted","created","fetchData","methods","console","log","get","then","res","status","$message","success","msg","obj","i","length","list","tutor_id","tutor_name","appointment_time_start","appointment_time_end","push","error","handleFullScreen","document","documentElement","requestFullscreen","handleCollapse","toMenu","$router","handleDelete","row","splice","forEach","item","number"],"sources":["src/views/FuDao/AppointmentList.vue"],"sourcesContent":["<template>\r\n  <Layout>\r\n\r\n        <el-main>\r\n          <el-card style=\" margin-bottom: 10px\" >\r\n              <div slot=\"header\" class=\"clearfix\">\r\n                  <span>我的预约</span>\r\n              </div>\r\n\r\n              <el-form :inline=\"true\" :model=\"query\" class=\"demo-form-inline\">\r\n                  <el-form-item label=\"辅导师姓名\">\r\n                      <el-input v-model=\"query.teacher_Name\" placeholder=\"请输入辅导师姓名\"></el-input>\r\n                  </el-form-item>\r\n                  <el-form-item label=\"辅导师工号\">\r\n                      <el-input v-model=\"query.teacher_ID\" placeholder=\"请输入辅导师工号\"></el-input>\r\n                  </el-form-item>\r\n                  <el-form-item label=\"辅导师学科\">\r\n                      <el-input v-model=\"query.teacher_Subject\" placeholder=\"请输入辅导师学科\"></el-input>\r\n                  </el-form-item>\r\n                  <el-form-item>\r\n                      <el-button type=\"primary\" @click=\"onSubmit\">查询</el-button>\r\n                  </el-form-item>\r\n                  <el-form-item>\r\n                      <el-button type=\"danger\" @click=\"toMenu\">返回</el-button>\r\n                  </el-form-item>\r\n              </el-form>\r\n\r\n              <el-table\r\n                :data=\"tableData\"\r\n                stripe\r\n                style=\"width: 100%; margin-bottom: 10px\">\r\n                <el-table-column\r\n                  prop=\"number\"\r\n                  label=\"序号\"\r\n                  width=\"150\"\r\n                  :align=\"'center'\">\r\n                  \r\n                </el-table-column>\r\n\r\n                <el-table-column\r\n                  prop=\"teacher_id\"\r\n                  label=\"老师编号\"\r\n                  width=\"200\"\r\n                  :align=\"'center'\">\r\n                </el-table-column>\r\n\r\n                <el-table-column\r\n                  prop=\"teacher_name\"\r\n                  label=\"老师姓名\"\r\n                  width=\"200\"\r\n                  :align=\"'center'\">\r\n                </el-table-column>\r\n\r\n                <el-table-column\r\n                  prop=\"appointment_time_start\"\r\n                  label=\"开始时间\"\r\n                  width=\"300\"\r\n                  :align=\"'center'\">\r\n                </el-table-column>\r\n\r\n                <el-table-column\r\n                  prop=\"appointment_time_end\"\r\n                  label=\"结束时间\"\r\n                  width=\"300\"\r\n                  :align=\"'center'\">\r\n                </el-table-column>\r\n                \r\n                <el-table-column\r\n                  label=\"操作\"\r\n                  \r\n                  :align=\"'center'\">\r\n                  <template slot-scope=\"scope\">\r\n                    <el-button @click=\"handleDelete(scope.$index, scope.row)\" type=\"text\" size=\"small\">取消预约</el-button>\r\n                  </template>\r\n                </el-table-column>\r\n              </el-table>\r\n              \r\n\r\n          </el-card>\r\n          <!-- 三个按钮 第一个跳转到选择辅导师的列表 第二个跳转到预约信息 第三个跳转到心理咨询活动-->\r\n          \r\n          <!-- 选择辅导师的列表 -->\r\n          \r\n          <!-- 点击按钮跳转到辅导师列表页面 -->\r\n          <!-- 辅导师列表页面显示所有辅导师的信息 -->\r\n          \r\n\r\n\r\n          \r\n\r\n        </el-main>\r\n</Layout>\r\n</template>\r\n\r\n<script>\r\nimport Layout from '@/components/Layout.vue'\r\nimport request from \"@/utils/request\";\r\nimport { formatDate } from \"@/utils/tool.js\";\r\nexport default {\r\n  name: 'LunTan',\r\n  components: {\r\n      Layout\r\n  },\r\n  data() {\r\n    return {\r\n\r\n      isCollapse: false,\r\n      asideWidth: '200px',\r\n      collapseIcon: 'el-icon-s-fold',\r\n\r\n      query: {\r\n        teacher_Name: '',\r\n        teacher_ID: '',\r\n        teacher_Subject: ''\r\n      },\r\n      \r\n      tableData:[],\r\n      \r\n      index: 0,\r\n      scheme: {},\r\n      \r\n    }\r\n  },\r\n  mounted() {\r\n\r\n  },\r\n  created() {\r\n    // 在组件创建后立即发送请求\r\n    this.fetchData();\r\n  },\r\n  methods: {\r\n    fetchData() {\r\n      console.log(\"fetchData111\")\r\n      request.get('/tutorial/appointments/get').then(res=>{\r\n        // console.log(\"交互成功\")\r\n        console.log(\"fetchDataDone\")\r\n        console.log(res)\r\n        if (res.status === 200) {\r\n          // console.log(\"收到后端回复200\")\r\n          \r\n          this.$message.success(res.msg)\r\n          let obj = res.data\r\n          for (let i = 0; i < obj.length; i++){\r\n\r\n            let list = {\r\n            // 老师编号 老师姓名 预约起止时间\r\n            'number': this.tableData.length + 1,\r\n            'teacher_id': obj[i].tutor_id,\r\n            'teacher_name': obj[i].tutor_name,\r\n            'appointment_time_start': formatDate(obj[i].appointment_time_start),\r\n            'appointment_time_end': formatDate(obj[i].appointment_time_end),\r\n            \r\n            }\r\n            this.tableData.push(list)\r\n          }\r\n          \r\n          // console.log(res.data)\r\n\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n        \r\n      })\r\n\r\n    },\r\n    handleFullScreen() {\r\n      document.documentElement.requestFullscreen()\r\n    },\r\n    handleCollapse() {\r\n      this.isCollapse = !this.isCollapse\r\n      this.asideWidth = this.isCollapse ? '64px' : '200px'\r\n      this.collapseIcon = this.isCollapse ? 'el-icon-s-unfold' : 'el-icon-s-fold'\r\n    },\r\n    \r\n    toMenu() {\r\n      this.$router.push('/fudao')\r\n    },\r\n    handleDelete(index, row) {\r\n      // 删除日程\r\n      this.tableData.splice(index, 1)\r\n      // 重新排序\r\n      this.tableData.forEach((item, index) => {\r\n        item.number = index + 1\r\n      })\r\n    }, \r\n    \r\n  }\r\n}\r\n</script>"],"mappings":";AA+FA,OAAAA,MAAA;AACA,OAAAC,OAAA;AACA,SAAAC,UAAA;AACA;EACAC,IAAA;EACAC,UAAA;IACAJ;EACA;EACAK,KAAA;IACA;MAEAC,UAAA;MACAC,UAAA;MACAC,YAAA;MAEAC,KAAA;QACAC,YAAA;QACAC,UAAA;QACAC,eAAA;MACA;MAEAC,SAAA;MAEAC,KAAA;MACAC,MAAA;IAEA;EACA;EACAC,QAAA,GAEA;EACAC,QAAA;IACA;IACA,KAAAC,SAAA;EACA;EACAC,OAAA;IACAD,UAAA;MACAE,OAAA,CAAAC,GAAA;MACApB,OAAA,CAAAqB,GAAA,+BAAAC,IAAA,CAAAC,GAAA;QACA;QACAJ,OAAA,CAAAC,GAAA;QACAD,OAAA,CAAAC,GAAA,CAAAG,GAAA;QACA,IAAAA,GAAA,CAAAC,MAAA;UACA;;UAEA,KAAAC,QAAA,CAAAC,OAAA,CAAAH,GAAA,CAAAI,GAAA;UACA,IAAAC,GAAA,GAAAL,GAAA,CAAAnB,IAAA;UACA,SAAAyB,CAAA,MAAAA,CAAA,GAAAD,GAAA,CAAAE,MAAA,EAAAD,CAAA;YAEA,IAAAE,IAAA;cACA;cACA,eAAAnB,SAAA,CAAAkB,MAAA;cACA,cAAAF,GAAA,CAAAC,CAAA,EAAAG,QAAA;cACA,gBAAAJ,GAAA,CAAAC,CAAA,EAAAI,UAAA;cACA,0BAAAhC,UAAA,CAAA2B,GAAA,CAAAC,CAAA,EAAAK,sBAAA;cACA,wBAAAjC,UAAA,CAAA2B,GAAA,CAAAC,CAAA,EAAAM,oBAAA;YAEA;YACA,KAAAvB,SAAA,CAAAwB,IAAA,CAAAL,IAAA;UACA;;UAEA;QAEA;UACA,KAAAN,QAAA,CAAAY,KAAA,CAAAd,GAAA,CAAAI,GAAA;QACA;MAEA;IAEA;IACAW,iBAAA;MACAC,QAAA,CAAAC,eAAA,CAAAC,iBAAA;IACA;IACAC,eAAA;MACA,KAAArC,UAAA,SAAAA,UAAA;MACA,KAAAC,UAAA,QAAAD,UAAA;MACA,KAAAE,YAAA,QAAAF,UAAA;IACA;IAEAsC,OAAA;MACA,KAAAC,OAAA,CAAAR,IAAA;IACA;IACAS,aAAAhC,KAAA,EAAAiC,GAAA;MACA;MACA,KAAAlC,SAAA,CAAAmC,MAAA,CAAAlC,KAAA;MACA;MACA,KA