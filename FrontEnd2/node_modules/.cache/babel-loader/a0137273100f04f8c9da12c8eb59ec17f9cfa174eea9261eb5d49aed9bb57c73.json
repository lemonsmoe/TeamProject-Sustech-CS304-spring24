ments/add', list).then(res=>{\r\n        console.log(res)\r\n        if (res.status === 200) {\r\n          console.log(res.msg)\r\n          // this.$router.push('/login')\r\n          // this.$message.success(res.msg)\r\n          this.addAppointment = false\r\n        } else {\r\n          console.log(res.msg)\r\n          // this.$message.error(res.msg)\r\n        }\r\n      })\r\n\r\n      // AppointmentList.tableData.push(list)\r\n\r\n      this.$message({\r\n        message: '创建成功',\r\n        type: 'success'\r\n      })\r\n      \r\n    },\r\n    cancelAdd() {\r\n      this.addAppointment = false\r\n    },\r\n    disabledDate(time) {\r\n      const today = new Date();\r\n      today.setHours(0, 0, 0, 0);\r\n      return time.getTime() < today.getTime();\r\n    },\r\n    handleTabClick(tab) {\r\n      console.log(`当前激活的选项卡: ${tab.name}`);\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n"],"mappings":";AA0HA,OAAAA,MAAA;AACA,OAAAC,OAAA;AACA,SAAAC,UAAA,EAAAC,UAAA;AACA;EACAC,IAAA;EACAC,UAAA;IACAL;EACA;EACAM,KAAA;IACA;MAEAC,UAAA;MACAC,UAAA;MACAC,YAAA;MACAC,IAAA;QACAN,IAAA;QACAO,MAAA;QACAC,KAAA;QACAC,KAAA;QACAC,QAAA;QACAC,IAAA;QACAC,QAAA;QACAC,IAAA;MACA;MACAC,KAAA;QACAC,YAAA;QACAC,UAAA;QACAC,eAAA;MACA;MACAC,cAAA;MACAC,eAAA;QACAC,UAAA;QACAC,YAAA;QACAC,YAAA;QACAC,gBAAA;QACAC,gBAAA;QACAC,MAAA;MACA;MACAC,SAAA;MAEAC,KAAA;MACAC,MAAA;MACAC,YAAA,IAEA;MACAC,UAAA,IAEA;MACAC,SAAA;IAEA;EACA;EACAC,QAAA,GAEA;EACAC,QAAA;IACA;IACA,KAAAC,SAAA;EACA;EACAC,OAAA;IACAD,UAAA;MACAE,OAAA,CAAAC,GAAA;MACA;MACAxC,OAAA,CAAAyC,GAAA,yBAAAC,IAAA,CAAAC,GAAA;QACA;QACAJ,OAAA,CAAAC,GAAA;QACAD,OAAA,CAAAC,GAAA,CAAAG,GAAA;QACA,IAAAA,GAAA,CAAAC,MAAA;UACA;;UAEA,KAAAC,QAAA,CAAAC,OAAA,CAAAH,GAAA,CAAAI,GAAA;UACA;QAEA;UACA,KAAAF,QAAA,CAAAG,KAAA,CAAAL,GAAA,CAAAI,GAAA;QACA;QACAR,OAAA,CAAAC,GAAA,CAAAG,GAAA,CAAAtC,IAAA;QACA,KAAA2B,YAAA,GAAAW,GAAA,CAAAtC,IAAA;MACA;;MAEA;MACAL,OAAA,CAAAyC,GAAA,4BAAAC,IAAA,CAAAC,GAAA;QACA;QACAJ,OAAA,CAAAC,GAAA;QACAD,OAAA,CAAAC,GAAA,CAAAG,GAAA;QACA,IAAAA,GAAA,CAAAC,MAAA;UACA;;UAEA,KAAA