   this.teacherform.name = ''\r\n      this.teacherform.subject = ''\r\n      this.teacherform.age = ''\r\n      this.teacherform.gender = ''\r\n      this.teacherform.email = ''\r\n\r\n    },\r\n    confirmAddTutor(){\r\n      // 确认添加（更新表格）\r\n      var list = {\r\n        'name': this.teacherform.name,\r\n        'subject': this.teacherform.subject,\r\n        'gender': this.teacherform.gender,\r\n        'age': this.teacherform.age,\r\n        'email': this.teacherform.email\r\n      }\r\n\r\n      request.post('/tutorial/tutors/add', list).then(res=>{\r\n        // console.log(\"交互成功\")\r\n        console.log(\"fetchDataDone\")\r\n        console.log(res)\r\n        if (res.status === 200) {\r\n          this.$message.success('添加成功')\r\n          this.getAllTutor()\r\n        } else {\r\n          this.$message.error(res.data.msg)\r\n        }\r\n        \r\n      })\r\n      \r\n      this.addteacher = false\r\n    },\r\n    \r\n    cancelAddTutor(){\r\n      // 取消添加\r\n      this.addteacher = false\r\n    },\r\n    getAllWorkshop() {\r\n      // 获取所有老师信息\r\n      request.get('/tutorial/workshops/get').then(res=>{\r\n        console.log(res);\r\n        if (res.status == 200) {\r\n          this.workshopData = res.data;\r\n          console.log(res.data);\r\n        }\r\n        else{\r\n\r\n        }\r\n        \r\n      })\r\n    },\r\n\r\n  },\r\n    \r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n\r\n</style>\r\n\r\n"],"mappings":"AAyFA,OAAAA,MAAA;AACA,OAAAC,OAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAH;EACA;EACAI,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,WAAA;MACAC,YAAA;MACAC,UAAA;MACAC,WAAA;QACAP,IAAA;QACAQ,OAAA;QACAC,GAAA;QACAC,MAAA;QACAC,KAAA;MACA;IAEA;EACA;EACAC,QAAA,GAEA;EACAC,QAAA;IACA;IACA,KAAAC,WAAA;EACA;EACAC,OAAA;IACAD,YAAA;MACA;MACAf,OAAA,CAAAiB,GAAA,yBAAAC,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA,IAAAA,GAAA,CAAAG,MAAA;UACA,KAAAjB,WAAA,GAAAc,GAAA,CAAAhB,IAAA;UACAiB,OAAA,CAAAC,GAAA,CAAAF,GAAA,CAAAhB,IAAA;QACA,OACA,CAEA;MAEA;IACA;IACAoB,eAAA;MACA;MACA,KAAAhB,