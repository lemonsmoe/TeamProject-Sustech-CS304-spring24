() {\r\n      // 获取所有老师信息\r\n      request.get('/tutorial/tutors/get').then(res=>{\r\n        console.log(res);\r\n        if (res.status == 200) {\r\n          this.teacherData = res.data;\r\n          console.log(res.data);\r\n        }\r\n        else{\r\n\r\n        }\r\n        \r\n      })\r\n    },\r\n    handleAddTutor() {\r\n      // 添加老师\r\n      this.addmessage = true\r\n\r\n      this.form.name = ''\r\n      this.form.subject = ''\r\n      this.form.age = ''\r\n      this.form.gender = ''\r\n      this.form.email = ''\r\n\r\n    },\r\n    confirmAddTutor(){\r\n      // 确认添加（更新表格）\r\n      var list = {\r\n        'name': this.form.name,\r\n        'subject': this.form.subject,\r\n        'gender': this.form.gender,\r\n        'age': this.form.age,\r\n        'email': this.form.email\r\n      }\r\n\r\n      request.post('/tutorial/tutors/add', list).then(res=>{\r\n        // console.log(\"交互成功\")\r\n        console.log(\"fetchDataDone\")\r\n        console.log(res)\r\n        if (res.status === 200) {\r\n          this.$message.success('添加成功')\r\n          this.getAllTutor()\r\n        } else {\r\n          this.$message.error(res.data.msg)\r\n        }\r\n        \r\n      })\r\n      \r\n      this.addmessage = false\r\n    },\r\n    \r\n    cancelAddTutor(){\r\n      // 取消添加\r\n      this.addmessage = false\r\n    },\r\n\r\n  },\r\n    \r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n\r\n</style>\r\n\r\n"],"mappings":"AA0KA,OAAAA,MAAA;AACA,OAAAC,OAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAH;EACA;EACAI,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,WAAA;MACAC,UAAA;MACAC,IAAA;QACAN,IAAA;QACAO,OAAA;QACAC,GAAA;QACAC,MAAA;QACAC,KAAA;MACA;IAEA;EACA;EACAC,QAAA,GAEA;EACAC,QAAA;IACA;IACA,KAAAC,WAAA;EACA;EACAC,OAAA;IACAD,YAAA;MACA;MACAd,OAAA,CAAAgB,GAAA,yBAAAC,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA,IAAAA,GAAA,CAAAG,MAAA;UACA,KAAAhB,WAAA,GAAAa,GAAA,CAAAf,IAAA;UACAgB,OAAA,CAAAC,GAAA,CAAAF,GAAA,CAAAf,IAAA;QACA,OACA,CAEA;MAEA;IACA;IACAmB,eAAA;MACA;MACA,KAAAhB,UAAA;MAEA,KAAAC,IAAA,CA