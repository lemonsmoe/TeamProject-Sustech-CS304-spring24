time': this.workshopform.time,\n        'location': this.workshopform.location\n      };\n      request.post('/tutorial/workshops/add', list).then(res => {\n        // console.log(\"交互成功\")\n        console.log(\"fetchDataDone\");\n        console.log(res);\n        if (res.status === 200) {\n          this.$message.success('添加成功');\n          this.getAllWorkshop();\n        } else {\n          this.$message.error(res.data.msg);\n        }\n      });\n      this.addworkshop = false;\n    },\n    cancelAddWorkshop() {\n      // 取消添加\n      this.addworkshop = false;\n    }\n  }\n};","map":{"version":3,"names":["Layout","request","Teacher","name","components","data","activeTab","teacherData","workshopData","addteacher","addworkshop","teacherform","subject","age","gender","email","workshopform","tutor_id","content","date","time","location","timeSlots","ageOptions","Array","from","length","_","i","mounted","created","getAllTutor","getAllWorkshop","methods","get","then","res","console","log","status","handleAddTutor","confirmAddTutor","$message","error","emailRegex","test","list","post","success","msg","cancelAddTutor","handleAddWorkshop","workshop","confirmAddWorkshop","cancelAddWorkshop"],"sources":["src/views/GuanLi/GuanLi.vue"],"sourcesContent":["<template>\r\n<Layout>\r\n  <el-main>\r\n    <el-card style=\" margin-bottom: 10px\" >\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <span>管理员</span>\r\n      </div>\r\n      <el-tabs v-model=\"activeTab\" >\r\n        <el-tab-pane label=\"辅导老师管理\" name=\"teacher\" class=\"classtype\"></el-tab-pane>\r\n        <el-tab-pane label=\"习题课管理\" name=\"class\" class=\"classtype\"></el-tab-pane>\r\n\r\n      </el-tabs>\r\n\r\n      <div v-if=\"activeTab === 'teacher'\">\r\n        <Teacher></Teacher>\r\n        \r\n      </div>\r\n\r\n      <div v-if=\"activeTab === 'class'\">\r\n        <el-button type=\"primary\" @click=\"handleAddWorkshop\">添加习题课</el-button>\r\n\r\n        <el-table :data=\"workshopData\" stripe sty