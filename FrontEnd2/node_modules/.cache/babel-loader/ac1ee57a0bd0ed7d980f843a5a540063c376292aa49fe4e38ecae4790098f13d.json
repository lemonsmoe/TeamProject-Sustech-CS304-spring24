\r\n        :data=\"tableData\"\r\n        stripe\r\n        style=\"width: 100%; margin-bottom: 10px\">\r\n        <el-table-column\r\n        type=\"index\"\r\n          label=\"序号\"\r\n          width=\"100\"\r\n          :align=\"'center'\">\r\n          \r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"ddl_type\"\r\n          label=\"类型\"\r\n          width=\"100\"\r\n          :align=\"'center'\">\r\n          \r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"ddl_time\"\r\n          label=\"开始时间\"\r\n          width=\"150\"\r\n          :align=\"'center'\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"ddl_title\"\r\n          label=\"事件名称\"\r\n          width=\"150\"\r\n          :align=\"'center'\">\r\n        </el-table-column>\r\n        \r\n        <el-table-column\r\n          prop=\"ddl_content\"\r\n          label=\"内容简介\"\r\n          width=\"200\"\r\n          :align=\"'center'\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"ddl_status\"\r\n          label=\"完成状态\"\r\n          width=\"150\"\r\n          :align=\"'center'\">\r\n        </el-table-column>\r\n        \r\n        <el-table-column\r\n          label=\"操作\"\r\n          width=\"168\"\r\n          :align=\"'center'\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button @click=\"handleEdit(scope.row)\" type=\"text\" size=\"small\">编辑</el-button>\r\n            <el-button @click=\"handleDelete(scope.$index, scope.row)\" type=\"text\" size=\"small\">删除</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <el-button type=\"primary\" @click=\"handleAdd\">添加新日程</el-button>\r\n      <el-button type=\"info\" @click=\"quitDetail\">完成</el-button>\r\n    \r\n    </el-dialog>\r\n\r\n    <!-- 添加新日程 -->\r\n    <el-dialog :visible.sync=\"addmessage\" width=\"600px\">\r\n      <el-form ref=\"form\" :model=\"form\" label-width=\"80px\">\r\n        <!-- <el-form-item label=\"开始时间\">\r\n          <el-input v-model=\"form.time\"></el-input>\r\n        </el-form-item> -->\r\n        <el-form-item label=\"开始时间\">\r\n          <el-time-picker\r\n            v-model=\"form.time\"\r\n            placeholder=\"选择时间\"\r\n            format=\"HH:mm:ss\"\r\n            value-format=\"HH:mm:ss\"\r\n          ></el-time-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"活动名称\">\r\n          <el-input v-model=\"form.name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"内容简介\">\r\n          <el-input v-model=\"form.content\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n\r\n      <el-button type=\"primary\" @click=\"confirmAdd\" :align=\"'center'\">立即创建</el-button>\r\n      <el-button type=\"info\" @click=\"cancelAdd\" :align=\"'center'\">取消</el-button>\r\n    </el-dialog>\r\n\r\n    <!-- 编辑日程 -->\r\n    <el-dialog :visible.sync=\"editmessage\" width=\"600px\">\r\n      <el-form ref=\"editForm\" :model=\"form\" label-width=\"80px\">\r\n        <!-- <el-form-item label=\"开始时间\">\r\n          <el-input v-model=\"editForm.time\"></el-input>\r\n        </el-form-item> -->\r\n        <el-form-item label=\"开始时间\">\r\n          <el-time-picker\r\n            v-model=\"editForm.time\"\r\n            placeholder=\"选择时间\"\r\n            format=\"HH:mm:ss\"\r\n            value-format=\"HH:mm:ss\"\r\n          ></el-time-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"活动名称\">\r\n          <el-input v-model=\"editForm.name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"内容简介\">\r\n          <el-input v-model=\"editForm.content\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n\r\n      <el-button type=\"primary\" @click=\"confirmEdit\" :align=\"'center'\">修改</el-button>\r\n      <el-button type=\"info\" @click=\"cancelEdit\" :align=\"'center'\">取消</el-button>\r\n    </el-dialog>\r\n\r\n  </Layout>\r\n\r\n</template>\r\n\r\n<script>\r\nimport Layout from '@/components/Layout.vue'\r\nimport request from \"@/utils/request\";\r\nimport { formatDate, formatTime } from \"@/utils/tool.js\";\r\nexport default {\r\n  name: 'RiLi',\r\n  components: {\r\n      Layout\r\n  },\r\n  data() {\r\n    return {\r\n      form: {\r\n        time: '',\r\n        name: '',\r\n        content: '',\r\n      },\r\n      editForm: {\r\n        time: '',\r\n        name: '',\r\n        content: '',\r\n      },\r\n      isCollapse: false,\r\n      asideWidth: '200px',\r\n      collapseIcon: 'el-icon-s-fold',\r\n      value: new Date(),\r\n      addmessage:false,\r\n      editmessage:false,\r\n      detail:false,\r\n      date:'',\r\n      row: {},\r\n      tableData: [],\r\n    }\r\n  },\r\n  mounted() {\r\n\r\n  },\r\n  created() {\r\n  // 在组件创建后立即发送请求\r\n    \r\n  },\r\n  methods: {\r\n    \r\n    handleFullScreen() {\r\n      document.documentElement.requestFullscreen()\r\n    },\r\n    handleCollapse() {\r\n      this.isCollapse = !this.isCollapse\r\n      this.asideWidth = this.isCollapse ? '64px' : '200px'\r\n      this.collapseIcon = this.isCollapse ? 'el-icon-s-unfold' : 'el-icon-s-fold'\r\n    },\r\n    chooseDay(date) {\r\n      // 查看详情\r\n      this.$message({\r\n        message: '选择的日期是: ' + date.day,\r\n        type: 'success'\r\n      })\r\n      this.detail = true\r\n      this.date = date.day\r\n      // console.log(date)\r\n      console.log(date.day)\r\n      \r\n      this.getDetail()\r\n\r\n    },\r\n    getDetail(){\r\n      // 获取日程安排\r\n      let list = {\r\n        'ddl_time': this.date\r\n      }\r\n      request.post('/calender/get_byday', list).then(res=>{\r\n        // console.log(\"交互成功\")\r\n        console.log(\"fetchDataDone\")\r\n        console.log(res)\r\n        if (res.status === 200) {\r\n          this.$message.success(res.data.msg)\r\n          console.log(res.data)\r\n          let ddls = res.data\r\n          for (let i = 0; i < ddls.length; i++) {\r\n            ddls[i].ddl_time = formatTime(ddls[i].ddl_time)\r\n          }\r\n          this.tableData = ddls\r\n\r\n        } else {\r\n          this.$message.error(res.data.msg)\r\n        }\r\n        \r\n      })\r\n    },\r\n    quitDetail(){\r\n      // 退出详情\r\n      this.detail = false\r\n    },\r\n\r\n    handleAdd(){\r\n      // 添加日程\r\n      this.addmessage = true\r\n\r\n      this.form.time = ''\r\n      this.form.name = ''\r\n      this.form.content = ''\r\n    },\r\n    confirmAdd(){\r\n      // 确认添加（更新表格）\r\n      var list = {\r\n        \r\n        ddl_time: this.date +\" \"+ this.form.time,\r\n        ddl_title: this.form.name,\r\n        ddl_content: this.form.content,\r\n      }\r\n\r\n      // this.tableData.push(list)\r\n\r\n      request.post('/calender/student_ddl/add', list).then(res=>{\r\n        // console.log(\"交互成功\")\r\n        console.log(\"fetchDataDone\")\r\n        console.log(res)\r\n        if (res.status === 200) {\r\n          this.$message.success(res.data.msg)\r\n\r\n          this.getDetail()\r\n\r\n        } else {\r\n          this.$message.error(res.data.msg)\r\n        }\r\n        \r\n      })\r\n\r\n      this.addmessage = false\r\n    },\r\n    cancelAdd(){\r\n      // 取消添加\r\n      this.addmessage = false\r\n    },\r\n    \r\n    \r\n    handleEdit(row){\r\n      // 编辑日程\r\n      this.addmessage = true\r\n      this.form.time = row.ddl_time\r\n      this.form.name = row.ddl_title\r\n      this.form.content = row.ddl_content\r\n      this.row = row\r\n\r\n    },\r\n    confirmEdit(){\r\n      // 确认编辑(创建+删除)\r\n      this.confirmAdd()\r\n      console.log(\"删除前\")\r\n      this.handleDelete(this.row)\r\n      console.log(\"删除后\")\r\n      this.addmessage = false\r\n    },\r\n    cancelEdit(){\r\n      // 取消编辑\r\n      this.addmessage = false\r\n    },\r\n\r\n    handleDelete(row) {\r\n      request.post('/calender/student_ddl/delete', row ).then(res => {\r\n        console.log(res)\r\n        if (res.status === 200) {\r\n          this.$message.success('删除成功')\r\n\r\n          this.getDetail()\r\n\r\n        } else {\r\n          this.$message.error(res.data.msg)\r\n        }\r\n      })\r\n    }, \r\n    \r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAkKA,OAAAA,MAAA;AACA,OAAAC,OAAA;AACA,SAAAC,UAAA,EAAAC,UAAA;AACA;EACAC,IAAA;EACAC,UAAA;IACAL;EACA;EACAM,KAAA;IACA;MACAC,IAAA;QACAC,IAAA;QACAJ,IAAA;QACAK,OAAA;MACA;MACAC,QAAA;QACAF,IAAA;QACAJ,IAAA;QACAK,OAAA;MACA;MACAE,UAAA;MACAC,UAAA;MACAC,YAAA;MACAC,KAAA,MAAAC,IAAA;MACAC,UAAA;MACAC,WAAA;MACAC,MAAA;MACAC,IAAA;MACAC,GAAA;MACAC,SAAA;IACA;EACA;EACAC,QAAA,GAEA;EACAC,QAAA;IACA;EAAA,CAEA;EACAC,OAAA;IAEAC,iBAAA;MACAC,QAAA,CAAAC,eAAA,CAAAC,iBAAA;IACA;IACAC,eAAA;MACA,KAAAlB,UAAA,SAAAA,UAAA;MACA,KAAAC,UAAA,QAAAD,UAAA;MACA,KAAAE,YAAA,QAAAF,UAAA;IACA;IACAmB,UAAAX,IAAA;MACA;MACA,KAAAY,QAAA;QACAC,OAAA,eAAAb,IAAA,CAAAc,GAAA;QACAC,IAAA;MACA;MACA,KAAAhB,MAAA;MACA,KAAAC,IAAA,GAAAA,IAAA,CAAAc,GAAA;MACA;MACAE,OAAA,CAAAC,GAAA,CAAAjB,IAAA,CAAAc,GAAA;MAEA,KAAAI,SAAA;IAEA;IACAA,UAAA;MACA;MACA,IAAAC,IAAA;QACA,iBAAAnB;MACA;MACAlB,OAAA,CAAAsC,IAAA,wBAAAD,IAAA,EAAAE,IAAA,CAAAC,GAAA;QACA;QACAN,OAAA,CAAAC,GAAA;QACAD,OAAA,CAAAC,GAAA,CAAAK,GAAA;QACA,IAAAA,GAAA,CAAAC,MAAA;UACA,KAAAX,QAAA,CAAAY,OAAA,CAAAF,GAAA,CAAAnC,IAAA,CAAAsC,GAAA;UACAT,OAAA,CAAAC,GAAA,CAAAK,GAAA,CAAAnC,IAAA;UACA,IAAAuC,IAAA,GAAAJ,GAAA,CAAAnC,IAAA;UACA,SAAAwC,CAAA,MAAAA,CAAA,GAAAD,IAAA,CAAAE,MAAA,EAAAD,CAAA;YACAD,IAAA,CAAAC,CAAA,EAAAE,QAAA,GAAA7C,UAAA,CAAA0C,IAAA,CAAAC,CAAA,EAAAE,QAAA;UACA;UACA,KAAA3B,SAAA,GAAAwB,IAAA;QAEA;UACA,KAAAd,QAAA,CAAAkB,KAAA,CAAAR,GAAA,CAAAnC,IAAA,CAAAsC,GAAA;QACA;MAEA;IACA;IACAM,WAAA;MACA;MACA,KAAAhC,MAAA;IACA;IAEAiC,UAAA;MACA;MACA,KAAAnC,UAAA;MAEA,KAAAT,IAAA,CAAAC,IAAA;MACA,KAAAD,IAAA,CAAAH,IAAA;MACA,KAAAG,IAAA,CAAAE,OAAA;IACA;IACA2C,WAAA;MACA;MACA,IAAAd,IAAA;QAEAU,QAAA,OAAA7B,IAAA,cAAAZ,IAAA,CAAAC,IAAA;QACA6C,SAAA,OAAA9C,IAAA,CAAAH,IAAA;QACAkD,WAAA,OAAA/C,IAAA,CAAAE;MACA;;MAEA;;MAEAR,OAAA,CAAAsC,IAAA,8BAAAD,IAAA,EAAAE,IAAA,CAAAC,GAAA;QACA;QACAN,OAAA,CAAAC,GAAA;